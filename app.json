{
  "expo": {
    "jsEngine": "hermes",
    "name": "boleia",
    "slug": "boleia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "boleia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.anania_augusto.boleia",
      "adaptiveIcon": {
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": false,
      "reactCompiler": false
    },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      }
    },
    "owner": "anania_augusto"
  }
}
