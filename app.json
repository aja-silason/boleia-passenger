{
  "expo": {
    "jsEngine": "hermes",
    "name": "driver.boleia",
    "slug": "driver-boleia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "driverboleia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.anania_augusto.driverboleia",
      "adaptiveIcon": {
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": false,
      "reactCompiler": false
    },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a73baeb4-36aa-43e7-a13b-bd4488728d9a"
      }
    },
    "owner": "anania_augusto"
  }
}
